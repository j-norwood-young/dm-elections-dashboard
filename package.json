{
  "name": "election-engine",
  "version": "0.9.1",
  "description": "A South African elections Wordpress plugin written for Daily Maverick",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Run tests from packages folders\" && exit 1",
    "start": "npm-run-all -p start:*",
    "dev": "npm-run-all -p dev:*",
