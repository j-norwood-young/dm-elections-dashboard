{
  "name": "dm-elections-dashboard",
  "version": "0.0.1",
  "description": "An elections dashboard monorepo for Daily Maverick",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Run tests from packages folders\" && exit 1",
    "start": "npm-run-all -p start:*",
    "dev": "npm-run-all -p dev:*",
    "start:iec-api": "cd packages/iec-api && npm start",
    "dev:iec-api": "cd packages/iec-api && npm run dev"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fontsource-variable/rubik": "^5.0.21",
    "npm-run-all": "^4.1.5"
  },
  "type": "module"
}
